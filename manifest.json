{
  "background": {
    "service_worker": "src/background.js"
  },
  "action": {
    "default_icon": {
      "19": "imgs/icon-19.png",
      "38": "imgs/icon-38.png"
    },
    "default_popup": "html/popup.html",
    "default_title": "Basic Chrome Extensionâ„¢"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "src/main.js"
      ],
      "css": [
        "css/main.css"
      ],
      "matches": [
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": {
    "extension_pages": "default-src 'self'"
  },
  "default_locale": "en",
  "description": "This is description of Basic Chrome Extension",
  "icons": {
    "128": "imgs/icon-128.png",
    "16": "imgs/icon-16.png"
  },
  "devtools_page": "html/devtools.html",
  "manifest_version": 3,
  "name": "Basic Chrome Extension",
  "host_permissions": [
    "<all_urls>"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "unlimitedStorage",
    "webNavigation",
    "webRequest",
    "alarms",
    "declarativeNetRequestFeedback",
    "declarativeNetRequest"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "src/*",
        "css/*"
      ],
      "matches": [
        "https://*/*"
      ]
    }
  ],
  "version": "1.1.0"
}
